    SUBROUTINE MD5(TEXT,HASH)
*
    GIT.FILENAME = 'MD5'
    GIT.REPO = 'https://github.com/Krowemoh/TCL-Utilities.git'
*
* VERSION
*
    VERSION = '1'
*
    @USER1 = 'MD5'
    @USER2 = 'MD5'
*
    EQU TRUE TO 1
    EQU FALSE TO 0
*
    HASH = ''
    STATUS = DIGEST('MD5',TEXT,1,MD5)
    STATUS = ENCODE('BASE64A',1,MD5,1,HASH,1)
*
    RETURN
*
* END OF PROGRAM
*
    END
*
