   SUBROUTINE RENDER(MAT ENV,ENV.SIZE,TEMPLATE,RESULT)
*
   GIT.FILENAME = 'RENDER'
   GIT.REPO = 'https://github.com/Krowemoh/TCL-Utilities.git'
*
* COMPILER DIRECTIVES
*
   $DEFINE DATABASE.QM
   $DEFINE PLATFORM.LINUX
*
   $IFDEF DATABASE.QM
      $CATALOGUE
   $ENDIF
*
   EQU TRUE TO 1
   EQU FALSE TO 0
*
   DIM ENV(ENV.SIZE)
*
   DIM FILES(1000)
   MAT FILES = ''
   FILE.CTR = 0
*
   MAP.CACHE = ''
   PARSE.CACHE = ''
   TRIGGER.STOP = FALSE
*
   RESULT = ''
   CALL RAW.RENDER(MAT ENV,ENV.SIZE,MAT FILES,FILE.CTR,TEMPLATE,RESULT,MAP.CACHE,PARSE.CACHE,TRIGGER.STOP)
*
   RETURN
*
* END OF PROGRAM
*
   END
*
   
   
   
